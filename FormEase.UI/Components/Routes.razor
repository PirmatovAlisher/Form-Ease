@using FormEase.UI.Components.Account.Shared
<Router AppAssembly="typeof(Program).Assembly">
	<Found Context="routeData">
		<AuthorizeRouteView RouteData="routeData" DefaultLayout="typeof(Layout.FormEaseLayout)">
			<NotAuthorized>
				<RedirectToLogin />
			</NotAuthorized>
		</AuthorizeRouteView>
		<FocusOnNavigate RouteData="routeData" Selector="h1" />
	</Found>
</Router>


